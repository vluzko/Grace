fn equality(a: i32, b: i32) -> boolean:
    return a == b

fn neq(a: i32, b: i32) -> boolean:
    return a != b

fn less(a: i32, b: i32) -> boolean:
    return a < b

fn lesse(a: i32, b: i32) -> boolean:
    return a <= b

fn great(a: i32, b: i32) -> boolean:
    return a > b

fn greate(a: i32, b: i32) -> boolean:
    return a >= b

