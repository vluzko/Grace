fn conditional(a, b):
    if 0:
        return a
    else:
        return b

fn add(a, b):
    let x = a + b
    return x

fn sub(a, b):
    let x = a - b
    return x

fn mult(a, b):
    let x = a * b
    return x

fn div(a, b):
    let x = a / b
    return x

fn loop():
    let x = 2
    while x:
        x = x - 1
    return x
