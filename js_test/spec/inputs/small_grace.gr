fn conditional(a: i32, b: i32) -> i32:
    if 0:
        return a
    else:
        return b

fn add(a: i32, b: i32) -> i32:
    let x = a + b
    return x

fn sub(a: i32, b: i32) -> i32:
    let x = a - b
    return x

fn mult(a: i32, b: i32) -> i32:
    let x = a * b
    return x

fn div(a: i32, b: i32) -> f64:
    let x = a / b
    return x

